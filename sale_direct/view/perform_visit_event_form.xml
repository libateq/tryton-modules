<form>
  <notebook>
    <page name="type">
      <label name="type"/>
      <field name="type"/>
    </page>
    <page string="Info" id="info">
      <label name="address"/>
      <field name="address" colspan="3"/>
      <label name="time"/>
      <field name="time"/>
      <label name="revisit_time"/>
      <field name="revisit_time"/>
      <separator name="notes" colspan="4"/>
      <field name="notes" colspan="4"/>
    </page>
    <page string="Sales Material" id="sales_material">
      <label name="warehouse"/>
      <field name="warehouse"/>
      <label name="location"/>
      <field name="location"/>
      <field name="sales_material_left" colspan="4"/>
      <field name="sales_material_collected" colspan="4"/>
      <field name="sales_material_lost" colspan="4"/>
    </page>
  </notebook>
</form>
